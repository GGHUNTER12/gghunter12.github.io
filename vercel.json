{
  "version": 2,
  "builds": [
    {
      "src": "s/index.js",   // Path to your 'index.js' inside the 's' folder
      "use": "@vercel/node"   // Specifies Vercel's Node.js runtime
    }
  ],
  "routes": [
    {
      "src": "/proxy",        // Match the /proxy route
      "dest": "s/index.js"    // Directs it to index.js inside the 's' folder
    },
    {
      "src": "/proxy/(.*)",   // Matches anything after "/proxy"
      "dest": "s/index.js"    // Directs it to index.js inside the 's' folder
    }
  ]
}
