{
  "version": 2,
  "builds": [
    {
      "src": "s/index.js",  // Make sure your file is inside the 's' folder
      "use": "@vercel/node"  // Tells Vercel to use Node.js runtime
    }
  ],
  "routes": [
    {
      "src": "/proxy",         // Matches any URL starting with "/proxy"
      "dest": "s/index.js"     // Directs it to the 'index.js' inside 's' folder
    },
    {
      "src": "/proxy/(.*)",    // Matches anything after "/proxy" and routes it to index.js
      "dest": "s/index.js"
    }
  ]
}
